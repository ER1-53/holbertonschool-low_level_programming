#include "main.h"

/**
 *_strncopy - return
 *@dest: is come back
 *@src: next
 *@n: again
 *
 *Description: it want eat me
 *
 *Return: Success
 */
char *_strncpy(char *dest, char *src, int n)
{
	int i;

	while (src[i] != '\0' && i < n)
	{
		dest[i] = src[i];
		i++;
	}
	dest[i] = '\0';
	return (dest);

}
